{
  "pages": {
    "admin/cms": {
      "path": "/admin/cms/*field"
    },
    "admin/cms/pages": {
      "path": "/admin/cms/pages"
    },
    "admin/cms/page-detail": {
      "path": "/admin/cms/pages/page/*pageId"
    },
    "admin/cms/storefront-home": {
      "path": "/admin/cms/storefront"
    },
    "admin/cms/storefront": {
      "path": "/admin/cms/storefront/*path"
    },
    "admin/cms/redirects": {
      "path": "/admin/cms/redirects"
    },
    "admin/cms/redirect-details": {
      "path": "/admin/cms/redirects/*id"
    },
    "admin/cms/settings": {
      "path": "/admin/cms/settings"
    }
  },
  "extensions": {
    "store/__overlay": {
      "component": "HighlightOverlay",
      "ssr": false
    },
    "admin/cms/__provider": {
      "component": "EditorProvider"
    },
    "admin/cms": {
      "component": "PagesAdmin"
    },
    "admin/cms/pages": {
      "component": "PageList"
    },
    "admin/cms/page-detail": {
      "component": "PageList"
    },
    "admin/cms/storefront-home": {
      "component": "PageEditor"
    },
    "admin/cms/storefront": {
      "component": "PageEditor"
    },
    "admin/cms/redirects": {
      "component": "RedirectList"
    },
    "admin/cms/redirect-details": {
      "component": "RedirectForm"
    },
    "admin/cms/settings": {
      "component": "Settings"
    },
    "admin/navigation/storeSetup/cms": {
      "component": "vtex.admin/InjectNavigationData",
      "props": {
        "subItems": [
          {
            "isNew": true,
            "labelId": "pages.admin-menu-button.storefront",
            "path": "/admin/cms/storefront"
          },
          {
            "isNew": true,
            "labelId": "pages.admin-menu-button.pageList",
            "path": "/admin/cms/pages"
          },
          {
            "isNew": true,
            "labelId": "pages.admin-menu-button.displayConditions",
            "path": "/admin/conditions-builder/pages"
          }
        ],
        "titleId": "appframe.navigation.cms.title"
      }
    }
  }
}
