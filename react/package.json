{
  "license": "UNLICENSED",
  "scripts": {
    "checkTypes": "tsc --noEmit",
    "ci": "run-p lint:locales test",
    "format": "prettier -l './**/*.+(json|css|md|graphql|ts|tsx)'",
    "lint": "eslint -c .eslintrc ./ --ext ts,tsx --max-warnings=0",
    "lint:locales": "intl-equalizer",
    "test": "vtex-test-tools test"
  },
  "lint-staged": {
    "**/*.+(json|css|md|graphql)": [
      "npm run lint:locales",
      "prettier --write",
      "git add"
    ],
    "**/*.+(ts|tsx)": [
      "eslint -c .eslintrc",
      "npm run lint:locales",
      "prettier --write",
      "git add"
    ]
  },
  "intl-equalizer": {
    "referenceLocale": "en",
    "localeDirectory": "../messages/"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "deep-object-diff": "^1.1.0",
    "draft-js": "^0.10.5",
    "draftjs-md-converter": "^1.2.0",
    "exenv": "^1.2.2",
    "json-schema-traverse": "^0.4.1",
    "keydown-from-click": "^1.1.1",
    "lodash": "^4.17.15",
    "ramda": "^0.25.0",
    "react-draggable": "^3.0.5",
    "react-dropzone": "^10.1.3",
    "react-jsonschema-form": "^1.0.1",
    "react-router-dom": "^5.0.0",
    "react-select": "^1.2.1",
    "react-sortable-hoc": "^1.9.1",
    "react-spring": "^8.0.18",
    "short-uuid": "^3.1.1",
    "url-parse": "^1.4.3",
    "vtex.ajv": "^6.10.4"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/draft-js": "^0.10.34",
    "@types/file-saver": "^2.0.0",
    "@types/graphql": "^14.2.3",
    "@types/intl": "^1.2.0",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/node": "^11.12.0",
    "@types/prop-types": "^15.5.2",
    "@types/ramda": "types/npm-ramda#dist",
    "@types/react": "^16.8.10",
    "@types/react-datepicker": "^2.2.1",
    "@types/react-dom": "^16.8.0",
    "@types/react-helmet": "^5.0.5",
    "@types/react-intl": "^2.3.17",
    "@types/react-jsonschema-form": "^1.0.7",
    "@types/react-router-dom": "^4.3.2",
    "@types/react-select": "1.2.1",
    "@types/url-parse": "^1.4.3",
    "@vtex/intl-equalizer": "^2.3.0",
    "@vtex/test-tools": "^1.0.0",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-client-preset": "^1.0.8",
    "babel": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-app": "^3.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-react-intl": "^2.0.0",
    "eslint": "^6.2.1",
    "eslint-config-vtex-react": "^5.0.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.1",
    "intl": "^1.2.5",
    "lint-staged": "^9.2.3",
    "mockdate": "^2.0.4",
    "moment": "^2.22.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react": "^16.8.1",
